import java.util.Scanner;

public class Aula1_teste5 {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        System.out.print("Escreva sua operacao:\n");
        String testestring = scanner.nextLine();

        while (!testestring.equals("=")) {
            
            System.out.print(testestring+"\n");
            testestring = scanner.nextLine();
        }

    }
}
